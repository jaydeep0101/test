<section class="meta-rana">
  <div class="metarana">
    <div class="container-fluid">
      <div class="metarana__inner">
        <div class = "metarana__box">
          <div class="row">
            <div class="col-12 col-md-3 col-lg-3">
              <div class="metarana__image">
              	<img src="{{section.settings.meta_rana_image  | product_img_url: "master"}}" alt="{{section.settings.meta_rana_image.alt}}">
              </div>
            </div>
            <div class="col-md-6 col-12  col-lg-3">
            	<div class="metarana__image">
              	<img src="{{section.settings.meta_rana_image  | product_img_url: "master"}}" alt="{{section.settings.meta_rana_image.alt}}">
              </div>
            </div>
            <div class="col-md-6 col-12 col-lg-6">
              <div class= "metarana__text">
                <div class="metarana__text--title">
                  <h2>{{section.settings.meta_rana_title}}</h2>
                </div>
                <div class="metarana__text--description">
                  <p class='common__font--philosophy'>{{section.settings.meta_rana_description}}</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>




{% schema %}
  {
    "name": "Our Philosophy ",
    "settings": [
		{
		"type": "image_picker",
		"id" : "meta_rana_image",
		"label" : "Meta Rana Image"
		},
		{
		"type": "text",
		"id" : "meta_rana_title",
		"label" : "Meta Rana Title"
		},
		{
		"type": "textarea",
		"id" : "meta_rana_description",
		"label" : "Meta Rana Description"
		}
	],
	"presets" : [
	{
	"name" : "Our Philosophy",
	"category" : "Philosophy"
	}
]
  }
{% endschema %}

